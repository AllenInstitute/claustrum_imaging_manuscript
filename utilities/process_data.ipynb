    "                session_type,\n",
    "                cell_id\n",
    "            ))\n",
    "            if len(rejected_cell_query) > 0:\n",
    "                cells_to_keep.drop(idx)\n",
    "\n",
    "        # save the list of cells to keep as an attribute of the session object\n",
    "        sessions[session_type][mouse].filtered_cell_IDs = cells_to_keep.values"
