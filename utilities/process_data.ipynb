    "    right_on=['mouse_id','recording_date'],\n",
    ")\n",
    "active_sessions_to_analyze['session_type'] = 'active'\n",
    "\n",
    "active_sessions_to_analyze['sex'] = active_sessions_to_analyze['mouse_id'].map(\n",
